{
  "name": "@rednight/components",
  "description": "Rednight Design system",
  "license": "AGPL-3.0",
  "author": "Rednight Inc.",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/components.js",
  "exports": {
    ".": {
      "import": "./dist/components.js"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build:watch": "tsc && vite build --watch",
    "build:storybook": "storybook build",
    "build": "tsc && vite build",
    "lint": "eslint lib",
    "pretty": "prettier --check $(find . -type d \\( -name 'node_modules' -o -name 'dist' \\) -prune -o \\( -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx' \\) -print)",
    "pretty:fix": "prettier --write $(find . -type d \\( -name 'node_modules' -o -name 'dist' \\) -prune -o \\( -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx' \\) -print)",
    "test": "vitest run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev --no-open -p 6006"
  },
  "dependencies": {
    "@rednight/react-polymorphic-types": "workspace:packages/react-polymorphic-types",
    "@rednight/shared": "workspace:packages/shared",
    "@rednight/styles": "workspace:packages/styles",
    "@rednight/utils": "workspace:packages/utils",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "ttag": "^1.8.6"
  },
  "devDependencies": {
    "@rednight/eslint-config-rednight": "workspace:packages/eslint-config-rednight",
    "@storybook/addon-docs": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/manager-api": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.0.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/text-encoding-utf-8": "^1.0.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "storybook": "^8.0.0",
    "text-encoding-utf-8": "^1.0.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-lib-inject-css": "^2.0.0",
    "vitest": "^1.4.0"
  }
}
