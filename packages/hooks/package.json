{
  "name": "@rednight/hooks",
  "description": "Generic business use-case agnostic helper hooks",
  "license": "AGPL-3.0",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/hooks.js",
  "exports": {
    ".": {
      "import": "./dist/hooks.js"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint lib",
    "pretty": "prettier --check $(find . -path ./node_modules -prune -o -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx')",
    "pretty:fix": "prettier --write $(find . -path ./node_modules -prune -o -type f -name '*.js' -o -name '*.ts' -o -name '*.tsx')",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rednight/eslint-config-rednight": "workspace:packages/eslint-config-rednight",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2"
  }
}
